	author = {Vafainia, Saeid and Breugelmans, Els and Bijmolt, Tammo},
	month = feb,
	year = {2019},
	keywords = {Call-to-action direct mailings, Customer heterogeneity, Monetary incentives, Non-monetary incentives, Purchase incidence, Retailing},
