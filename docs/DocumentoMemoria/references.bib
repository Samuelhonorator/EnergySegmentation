	author = {Pedersen, Lasse Heje and Fitzgibbons, Shaun and Pomorski, Lukasz},
	month = nov,
	year = {2021},
	note = {Place: Lausanne
Publisher: Elsevier Science Sa
WOS:000709772800005},
	keywords = {Carbon, Governance, Impact investing, Portfolio choice, Socially responsible investing, Sustainable investing, accruals, corporate, csr, esg, governance, order imbalance},
	pages = {572--597},
}

@article{chowdhry_investing_2019,
