@article{li_information_2009,
	title = {Information structures with unawareness},
	volume = {144},
	issn = {0022-0531},
	url = {http://www.sciencedirect.com/science/article/pii/S0022053108001373},
	doi = {10.1016/j.jet.2008.10.001},
	abstract = {I construct a state space model with unawareness following [R.J. Aumann, Agreeing to disagree, Ann. Stat. 76 (1976) 1236–1239]. Dekel et al. [E. Dekel, B.L. Lipman, A. Rustichini, Standard state-space models preclude unawareness, Econometrica 66 (1998) 159–173] show that standard state space models are incapable of representing unawareness. The model circumvents the impossibility result by endowing the agent with a subjective state space that differs from the full state space when he has the unawareness problem. Information is modeled as a pair, consisting of both factual information and awareness information. The model preserves the central properties of the standard information partition model.},
