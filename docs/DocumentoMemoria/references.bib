	author = {Coupet, Jason and Berrett, Jessica L.},
	year = {2019},
	keywords = {administrative ratio, data envelopment analysis, efficiency, overhead ratio, stochastic frontier analysis},
	pages = {299--320},
}

@article{verbic_econometric_2009,
