@article{nielsen_cultural_2003,
	title = {Cultural policy and evaluation of quality},
	volume = {9},
	issn = {10286632},
	url = {http://search.ebscohost.com/login.aspx?direct=true&db=asn&AN=11985177&lang=es&site=ehost-live},
	doi = {10.1080/1028663032000161678},
	abstract = {Based on Danish experiences the article discusses the increasing role that the discourse of "quality" is playing in cultural policy. Throughout the 1990s, from the general cultural debate, this discourse has spread to more instrumental considerations and practices in the field of cultural policy. The article presents this process in a historical perspective and characterises the present tendency towards a technocratisation of the discourse of quality under the auspices of New Public Management and the general trend in modern state policy towards instrumentalising all societal resources in the global economic competition. Opposing this line of development, it is argued that "quality" could serve as a progressive, non-instrumentalist discourse if it is defined in relation to a modernised understanding of the "rational core" of the cultural policy tradition of the Nordic countries. In this perspective, the article suggests a contextualisation of the concept of quality and possible ways of implementing the discourse in practical cultural policy.},
	number = {3},
	urldate = {2021-05-30},
	journal = {International Journal of Cultural Policy},
	author = {Nielsen, Henrik Kaare},
	month = nov,
	year = {2003},
