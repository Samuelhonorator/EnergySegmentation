@article{cerreia-vioglio_multinomial_2021,
	title = {Multinomial logit processes and preference discovery: inside and outside the black box},
	shorttitle = {Multinomial logit processes and preference discovery},
	url = {http://arxiv.org/abs/2004.13376},
	abstract = {We provide two characterizations, one axiomatic and the other neuro-computational, of the dependence of choice probabilities on deadlines, within the widely used softmax representation {\textbackslash}[ p\_\{t\}{\textbackslash}left( a,A{\textbackslash}right) ={\textbackslash}dfrac\{e{\textasciicircum}\{{\textbackslash}frac\{u{\textbackslash}left( a{\textbackslash}right) \}\{{\textbackslash}lambda {\textbackslash}left( t{\textbackslash}right) \}+{\textbackslash}alpha {\textbackslash}left( a{\textbackslash}right) \}\}\{{\textbackslash}sum\_\{b{\textbackslash}in A\}e{\textasciicircum}\{{\textbackslash}frac\{u{\textbackslash}left( b{\textbackslash}right) \}\{{\textbackslash}lambda {\textbackslash}left( t{\textbackslash}right) \}+{\textbackslash}alpha {\textbackslash}left( b{\textbackslash}right) \}\}\% {\textbackslash}] where \$p\_\{t\}{\textbackslash}left( a,A{\textbackslash}right) \$ is the probability that alternative \$a\$ is selected from the set \$A\$ of feasible alternatives if \$t\$ is the time available to decide, \${\textbackslash}lambda\$ is a time dependent noise parameter measuring the unit cost of information, \$u\$ is a time independent utility function, and \${\textbackslash}alpha\$ is an alternative-specific bias that determines the initial choice probabilities reflecting prior information and memory anchoring. Our axiomatic analysis provides a behavioral foundation of softmax (also known as Multinomial Logit Model when \${\textbackslash}alpha\$ is constant). Our neuro-computational derivation provides a biologically inspired algorithm that may explain the emergence of softmax in choice behavior. Jointly, the two approaches provide a thorough understanding of soft-maximization in terms of internal causes (neurophysiological mechanisms) and external effects (testable implications).},
	urldate = {2021-05-11},
	journal = {arXiv:2004.13376 [econ, q-bio]},
	author = {Cerreia-Vioglio, Simone and Maccheroni, Fabio and Marinacci, Massimo and Rustichini, Aldo},
	month = jan,
	year = {2021},
	note = {arXiv: 2004.13376},
	keywords = {Economics - Theoretical Economics, Quantitative Biology - Neurons and Cognition},
}

