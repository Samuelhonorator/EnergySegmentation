	doi = {10.1016/bs.hesbe.2018.11.001},
	keywords = {Arrow–Debreu, Attention, Consumer theory, Heuristics and biases, Inertia, Overreaction, Salience, Shrouding, Sparsity, Underreaction},
	pages = {261--343},
}

