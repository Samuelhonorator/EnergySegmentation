	number = {5},
	urldate = {2020-03-19},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Cárdenas, Juan-Camilo and Janssen, Marco A. and Ale, Manita and Bastakoti, Ram and Bernal, Adriana and Chalermphol, Juthathip and Gong, Yazhen and Shin, Hoon and Shivakoti, Ganesh and Wang, Yibo and Anderies, John M.},
	month = jan,
	year = {2017},
	pmid = {28096383},
	note = {Publisher: National Academy of Sciences
Section: Social Sciences},
	keywords = {commons, framed field experiments, irrigation, public goods, risk},
	pages = {921--925},
}

@article{secilmis_role_2019,
